            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="#">@ViewBag.Title</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">


             
              <li>@Html.ActionLink("Alunos", "Index", "Aluno")</li>
              <li>@Html.ActionLink("Cursos", "Index", "Curso")</li>
              <li>@Html.ActionLink("Professores", "Index", "Professor")</li>
              <li>@Html.ActionLink("Departamentos", "Index", "Departamento")</li>


              @*<li class="active"><a href="#">Home</a></li>
              <li><a href="#about">About</a></li>
              <li><a href="#contact">Contact</a></li>*@

              <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">Templates <b class="caret"></b></a>
              <ul class="dropdown-menu">
                <li><a href="#">Básicos</a></li>
                <li><a href="#">Intermediários</a></li>
                <li><a href="#">Avançados</a></li>
                <li class="divider"></li>
                <li class="dropdown-header">Componentes</li>
                <li><a href="#">Formulários</a></li>
                <li><a href="#">Extras</a></li>
              </ul>
            </li>
          </ul>
        </div><!--/.nav-collapse -->
